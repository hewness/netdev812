<Window x:Class="Addresses.frmPrint"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Print" Height="143" Width="300">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="1,1" MappingMode="RelativeToBoundingBox" StartPoint="0,0">
                <GradientStop Color="Black" Offset="1"/>
                <GradientStop Color="#FF012DF3" Offset="0.318"/>
            </LinearGradientBrush>
        </Grid.Background>

        <Button Content="Cancel" HorizontalAlignment="Left" Margin="110,81,0,0" VerticalAlignment="Top" Width="75" Click="btnCancel_Click"/>
        <ProgressBar x:Name="pBar" HorizontalAlignment="Left" Height="27" Margin="10,31,0,0" VerticalAlignment="Top" Width="272"/>

    </Grid>
</Window>
